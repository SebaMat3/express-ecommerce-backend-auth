{
  "name": "express-store",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch api/index.js",
    "prebuild": "npm run migrations:run:prod",
    "start": "node api/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "migrations:run:prod": "npx sequelize-cli db:migrate --env production",
    "migrations:revert:prod": "npx sequelize-cli db:migrate:undo --env production",
    "migrations:delete:prod": "npx sequelize-cli db:migrate:undo:all --env production --config ./api/db/config.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.5",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  }
}
